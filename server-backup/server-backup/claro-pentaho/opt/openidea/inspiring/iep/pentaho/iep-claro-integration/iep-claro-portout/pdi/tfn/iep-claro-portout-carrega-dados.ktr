    <sql>select pap.nu_telefone NU_TELEFONE,&#xd;&#xa;           phsp.dt_alteracao_status DT_ALTERACAO_STATUS,&#xd;&#xa;           pap.cd_status_ea STATUS,&#xd;&#xa;       pap.dt_agendamento_portabilidade DT_AGENDAMENTO_PORTOUT,&#xd;&#xa;       prxs.no_operadora OPERADORA,&#xd;&#xa;       pap.cd_spid_receptora COD_OPERADORA,&#xd;&#xa;           pd.cd_tipo_assinatura TIPO&#xd;&#xa;from gtaportno.portnum_andamento_processo pap&#xd;&#xa;     LEFT JOIN gtaportno.portnum_hist_status_processo phsp on pap.nu_bilhete_claro &#x3d; phsp.nu_bilhete_claro&#xd;&#xa;        and pap.cd_status_processo &#x3d; phsp.cd_status_atual&#xd;&#xa;        and pap.dt_status_processo&#x3d;phsp.dt_alteracao_status&#xd;&#xa;     LEFT JOIN gtaportno.portnum_rn1_x_spid prxs on pap.cd_spid_receptora&#x3d;prxs.cd_spid&#xd;&#xa;         LEFT JOIN gtaportno.portnum_doacao pd on pap.nu_bilhete_claro &#x3d; pd.nu_bilhete_claro&#xd;&#xa;  where phsp.dt_alteracao_status&#xd;&#xa;  BETWEEN TO_DATE &#x28;&#x27;&#x24;&#x7b;iep.claro.portout.date.start&#x7d; &#x24;&#x7b;iep.claro.portout.time.start&#x7d;&#x27;, &#x27;DD&#x2f;MM&#x2f;YYYY HH24&#x3a;MI&#x3a;SS&#x27;&#x29;&#xd;&#xa;  AND TO_DATE &#x28;&#x27;&#x24;&#x7b;iep.claro.portout.date.end&#x7d; &#x24;&#x7b;iep.claro.portout.time.end&#x7d;&#x27;, &#x27;DD&#x2f;MM&#x2f;YYYY HH24&#x3a;MI&#x27;&#x29;&#xd;&#xa;     and pap.cd_tipo_processo &#x3d; &#x27;DO&#x27;&#xd;&#xa;     and pd.cd_tipo_assinatura &#x3d; &#x27;PRE&#x27;&#xd;&#xa;     and pap.cd_status_ea&#xd;&#xa;     in &#x28;&#x27;active&#x27;,&#x27;old&#x27;,&#x27;download-failed-partial&#x27;,&#x27;pending&#x27;,&#x27;cancelled&#x27;&#x29;&#xd;&#xa;	 and pap.cd_spid_receptora&#xd;&#xa;	 in &#x28;&#x27;0286&#x27;,&#x27;0351&#x27;,&#x27;0377&#x27;,&#x27;0314&#x27;,&#x27;0331&#x27;,&#x27;0114&#x27;,&#x27;0112&#x27;,&#x27;0312&#x27;,&#x27;0302&#x27;,&#x27;0143&#x27;,&#x27;0343&#x27;,&#x27;0306&#x27;,&#x27;0341&#x27;,&#x27;0141&#x27;,&#x27;0215&#x27;,&#x27;0320&#x27;&#x29;&#xd;&#xa;group by pap.nu_telefone,&#xd;&#xa;           phsp.dt_alteracao_status,&#xd;&#xa;           pap.cd_status_ea,&#xd;&#xa;       pap.dt_agendamento_portabilidade,&#xd;&#xa;       prxs.no_operadora,&#xd;&#xa;       pap.cd_spid_receptora,&#xd;&#xa;           pd.cd_tipo_assinatura&#xd;&#xa;order by pap.nu_telefone desc</sql>
    <limit>0</limit>
