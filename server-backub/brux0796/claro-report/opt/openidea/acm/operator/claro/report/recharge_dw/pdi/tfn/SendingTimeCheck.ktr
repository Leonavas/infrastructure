        <jsScript_name>weekDay</jsScript_name>
        <jsScript_script>var _SENDING_TIME_FG &#x3d; SENDING_TIME_FG&#x3b;&#xd;&#xa;&#xd;&#xa;if &#x28;parseInt&#x28;SENDING_TIME_FG&#x29; &#x3d;&#x3d; 1&#x29; &#x7b;&#xd;&#xa;&#xd;&#xa;    var day &#x3d; date2str&#x28;EXEC_TIME, &#x22;EEEE&#x22;, &#x22;EN&#x22;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;    if &#x28;&#x22;Saturday&#x22;.equals&#x28;day&#x29;&#x29; &#x7b;&#xd;&#xa;        var hour &#x3d; date2str&#x28;EXEC_TIME,&#x22;HH&#x22;&#x29;&#x3b;&#xd;&#xa;        if &#x28;str2num&#x28;hour&#x29; &#x3c; 9 &#x7c;&#x7c; str2num&#x28;hour&#x29; &#x3e; 13&#x29; &#x7b;&#xd;&#xa;            _SENDING_TIME_FG &#x3d; 0&#x3b;&#xd;&#xa;        &#x7d;&#xd;&#xa;    &#x7d; else if &#x28;&#x22;Sunday&#x22;.equals&#x28;day&#x29;&#x29; &#x7b;&#xd;&#xa;        _SENDING_TIME_FG &#x3d; 0&#x3b;&#xd;&#xa;    &#x7d; else &#x7b;&#xd;&#xa;        var hour &#x3d; date2str&#x28;EXEC_TIME,&#x22;HH&#x22;&#x29;&#x3b;&#xd;&#xa;        if &#x28;str2num&#x28;hour&#x29; &#x3c; 9 &#x7c;&#x7c; str2num&#x28;hour&#x29; &#x3e; 20&#x29; &#x7b;&#xd;&#xa;            _SENDING_TIME_FG &#x3d; 0&#x3b;&#xd;&#xa;        &#x7d;&#xd;&#xa;    &#x7d;&#xd;&#xa;&#x7d;</jsScript_script>
