    <sql>SELECT CASE WHEN TO_CHAR&#x28;t.num_sequencial&#x29; &#x3d; &#x27;-1&#x27; THEN t.cod_cartao ELSE TO_CHAR&#x28;t.num_sequencial&#x29; END AS SQ_RECARGA,&#xd;&#xa;       t.num_ntc AS NU_MSISDN,&#xd;&#xa;       CASE c.dsc_tipo_credito &#xd;&#xa;           WHEN &#x27;Pin Eletronico&#x27; THEN &#x27;PIN eletronico&#x27; &#xd;&#xa;           WHEN &#x27;Cart&#xe3;o F&#xed;sico&#x27; THEN &#x27;Cartao fisico&#x27; &#xd;&#xa;           WHEN &#x27;Online&#x27; THEN &#x27;Recarga online&#x27; &#xd;&#xa;           ELSE c.dsc_tipo_credito END AS CD_TIPO_RECARGA,&#xd;&#xa;       TO_CHAR&#x28;t.dat_insercao_credito, &#x27;YYYYMMDD&#x27;&#x29; AS DT_RECARGA,&#xd;&#xa;       LPAD&#x28;t.hor_insercao_credito, 6, &#x27;0&#x27;&#x29; AS HR_RECARGA,&#xd;&#xa;       t.val_credito_inserido AS VL_VOUCHER,&#xd;&#xa;       gc.cod_grupo_cartao AS CD_GRUPO_CARTAO,&#xd;&#xa;       &#x27;R&#x27; AS PC_PACOTE&#xd;&#xa;  FROM dwh.bi_fp_assinante_credito t&#xd;&#xa; INNER JOIN dwh.bi_dim_grupo_cartao gc&#xd;&#xa;    ON t.dw_grupo_cartao &#x3d; gc.dw_grupo_cartao&#xd;&#xa; INNER JOIN dwh.bi_dim_tipo_credito c&#xd;&#xa;    ON t.cod_tipo_credito &#x3d; c.cod_tipo_credito&#xd;&#xa; WHERE t.dat_insercao_credito &#x3d; TO_DATE&#x28;&#x27;&#x24;&#x7b;acm.tfn.date.prc&#x7d;&#x27;,&#x27;YYYYMMDD&#x27;&#x29;&#xd;&#xa;   AND gc.dsc_categoria_grupo_cartao &#x3d; &#x27;Recarga&#x27;&#xd;&#xa;   AND TRIM&#x28;gc.dsc_grupo_cartao_wpp&#x29; NOT IN &#x28;&#x27;Pl. Cont Int&#x27;,&#x27;Pl. Controle&#x27;,&#x27;Testes TI&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
    <limit>0</limit>
