        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.*;
import java.util.regex.*;

private FieldHelper inMsisdn = null;
private FieldHelper infoDDD = null;
private FieldHelper infoMask = null;
private FieldHelper infoLength = null;
private Map ndcDefinitionMap = null;

private class NdcDefinition {
    private int length;
    private Pattern pattern;

    public NdcDefinition(int length, String mask) {
        this.length = length;
        this.pattern = Pattern.compile(mask);
    }

    public int getLength() {
        return this.length;
    }

    public boolean matches(String sn) {
        return this.pattern.matcher(sn).matches();
    }
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    if (first){
        first = false;

        ndcDefinitionMap = new HashMap(100);

        inMsisdn = get(Fields.In, "MSISDN");

        infoDDD = get(Fields.Info, "DDD");
        infoMask = get(Fields.Info, "MASK");
        infoLength = get(Fields.Info, "LENGTH");

        RowSet infoStream = findInfoRowSet("NDC_stream_tag");

        Object[] infoRow = null;

        int infoRowCount = 0;

        while((infoRow = getRowFrom(infoStream)) != null){
            infoRowCount++;

            NdcDefinition ndcDefinition = new NdcDefinition(infoLength.getInteger(infoRow).intValue(), infoMask.getString(infoRow));
            ndcDefinitionMap.put(infoDDD.getString(infoRow), ndcDefinition);
        }
    }

    Object[] r = getRow();

    if (r == null) {
      setOutputDone();
      return false;
    }

    r = createOutputRow(r, data.outputRowMeta.size());

    String msisdn = inMsisdn.getString(r);

    if (msisdn == null || msisdn.length() < 3) {
        return true;
    }

    String ndc = msisdn.substring(0,2);
    String sn = msisdn.substring(2);
    NdcDefinition ndcDefinition = (NdcDefinition) ndcDefinitionMap.get(ndc);

    if (ndcDefinition == null) {
        return true;
    }

    if (ndcDefinition.getLength() == msisdn.length()) {
        if (!ndcDefinition.matches(sn)) {
            return true;
        }
    } else if (ndcDefinition.getLength() == 11 && msisdn.length() == 10) {
        if (ndcDefinition.matches("9" + sn)) {
            inMsisdn.setValue(r, ndc + "9" + sn);
        } else {
            return true;
        }
    } else if (ndcDefinition.getLength() == 10 && msisdn.length() == 11) {
        if ("9".equals(sn.substring(0, 1)) && ndcDefinition.matches(sn.substring(1))) {
            inMsisdn.setValue(r, ndc + sn.substring(1));
        } else {
            return true;
        }
    } else {
        return true;
    }

    putRow(data.outputRowMeta, r);
    return true;
}
]]></class_source>
        </definition>
